    <span>{{ model.name | titlecase() }}</span>
    <span class="arrow">
      
    {{ html: expanded ? "&#x25BC;" : "&#x25B6;" }}
    </span>
  </div>

  {{#if: expanded}}
    {{ html: ~sections.articles }}
  {{/}}

</li>

